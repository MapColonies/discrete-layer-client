    "@turf/convex": "^6.0.3",
    "@turf/distance": "6.2.0-alpha.2",
    "@turf/helpers": "^6.1.4",
    "@turf/intersect": "^6.1.3",
    "@turf/kinks": "^6.5.0",
    "@turf/linestring-to-polygon": "^5.0.0",
    "@turf/mask": "^6.5.0",
    "@turf/polygon-to-line": "^6.0.3",
    "@turf/simplify": "^6.5.0",
    "@turf/square-grid": "^6.5.0",
    "@turf/truncate": "^6.5.0",
    "@types/convert-units": "^2.3.11",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^27.5.2",
    "@types/jsum": "^0.1.0",
    "@types/lodash": "4.14.165",
    "@types/node": "^16.18.38",
    "@types/react": "^18.2.14",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^18.2.6",
    "@types/react-sortable-tree": "0.3.13",
    "@types/shpjs": "^3.4.1",
    "@types/topojson-client": "^3.1.5",
    "@types/topojson-server": "^3.0.4",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^9.0.1",
    "ag-grid-community": "24.1.0",
    "ag-grid-react": "24.1.0",
    "assert": "^2.0.0",
    "axios": "^0.21.1",
